      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "kkvh4vtnk"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "query": "FinalOutput = [\"Mission\", \"Date\", \"Hour\", \"Type\", \"Count\", \"Size\", \"production_latency_min\", \r\n  \"production_latency_max\", \"production_latency_avg\", \"total_latency_min\", \"total_latency_max\", \"total_latency_avg\"]\r\n\r\nRawSeries = from(bucket: \"production_hour\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"product\" and r._field =~ /mission|date|hour|type|count|size|production_latency_min|production_latency_max|production_latency_avg|total_latency_min|total_latency_max|total_latency_avg/)\r\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\r\n\r\n//  |> filter(fn: (r) => r.type == \"${var_types}\")\r\n\r\n\r\n NamedSeries = RawSeries\r\n  |> map(fn: (r) => ({\"Mission\":r.mission, \"Date\":r.date, \"Hour\":r.hour, \"Type\":r.type, \"Count\":r.count, \"Size\":r.size, \r\n        \"production_latency_min\":r.production_latency_min, \"production_latency_max\":r.production_latency_max, \"production_latency_avg\":r.production_latency_avg, \r\n        \"total_latency_min\":r.total_latency_min, \"total_latency_max\":r.total_latency_max, \"total_latency_avg\":r.total_latency_avg}))\r\n    |> keep(columns:FinalOutput)\r\n    \r\nNamedSeries |> yield()",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  mission.code AS \"Mission\", \r\n  product_class.product_type AS \"Product Type\",\r\n  product_file_name AS \"Product File Name\",\r\n  file_size AS \"File Size\",\r\n  \r\n  CASE\r\n    WHEN production_type = 'SYSTEMATIC' THEN 'Systematic Production'\r\n    WHEN production_type = 'ON_DEMAND_DEFAULT' THEN 'On-Demand Default'\r\n    WHEN production_type = 'ON_DEMAND_NON_DEFAULT' THEN 'On-Demand Non-Default'\r\n    ELSE production_type\r\n  END \r\n  AS \"Production Type\",\r\n  \r\n  sensing_start_time at TIME zone 'UTC' AS \"Sensing Start Time\",\r\n  sensing_stop_time  at TIME zone 'UTC' AS \"Sensing Stop Time\",\r\n  raw_data_availability_time at TIME zone 'UTC' AS \"Raw Data Availability Time\",\r\n  generation_time at TIME zone 'UTC' AS \"Generation Time\",\r\n  publication_time at TIME zone 'UTC' AS \"Publication Time\",\r\n  \r\n  CASE WHEN raw_data_availability_time IS NULL \r\n    THEN publication_time at TIME zone 'UTC' - sensing_stop_time at TIME zone 'UTC'\r\n    ELSE publication_time  at TIME zone 'UTC' - raw_data_availability_time at TIME zone 'UTC'  \r\n  END AS \"Production Latency\",\r\n  \r\n  publication_time at TIME zone 'UTC' - sensing_stop_time at TIME zone 'UTC' AS \"Total Latency\"\r\nFROM product\r\nLEFT JOIN product_file ON product.id = product_file.product_id\r\nINNER JOIN product_class ON product_class.id = product.product_class_id\r\nINNER JOIN mission ON mission.id = product_class.mission_id AND \r\n  product_class.processing_level IS NOT NULL\r\nWHERE \r\n  production_type IN (${var_production_type}) AND\r\n  $__timeFilter(publication_time at TIME zone 'UTC')\r\nORDER BY publication_time at TIME zone 'UTC'",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Products ($var_production_type)",
